{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/generate-data",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/generate-data"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "/"
    }
  ]
}
